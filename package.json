{
  "name": "ele-101",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "electron-is-dev": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "wait-on": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "electron-devtools-installer": "^3.1.1",
    "html-webpack-plugin": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.0.3",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.main.js --mode development && yarn start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack.render.js --mode development",
    "electron:serve": "wait-on http-get://localhost:4000/ && yarn dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack.main.js",
    "build:react": "webpack --config webpack.render.js",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --dir"
  }
}
